    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.24.0",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.6",
    "vite": "^6.2.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
